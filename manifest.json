{
  "manifest_version": 2,

  "name": "Wakapedia",
  "description": "This extension redirects the browser to Wakapedia when visiting Wikipedia",
  "version": "1.0",

  "background": {
    "scripts": [
        "redirect.js"
    ]
  },
  "icons": {
    "16": "wikilogo16.png",
    "48": "wikilogo48.png",
    "128": "wikilogo128.png"
  },

  "permissions": [
    "webRequest",
    "*://en.wikipedia.org/wiki/*",
    "webRequestBlocking"
  ]
}
